<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<table>
  <thead>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.watched_on%></td>
        <td><%= post.theater %></td>
        <td><%= post.comment %></td>
      </tr>
    <% end %>
  </tbody>
</table>